{% extends "templates/base.jinja" %}
{% block content %}
    {% for post in posts %}
        <div class="post">
            <p> time: {{ post.timePosted }} </p>
            <p> author: {{ post.author }} </p>
            <p> title: {{ post.title }} </p>
            <p> content: {{ post.content }} </p>
        </div>
    {% endfor %}
{% endblock content %}